{
	"build": {
		"dockerfile": "Dockerfile"
	},
	"features": {},
	"hostRequirements": {
		"cpus": 16,
		"memory": "8gb",
		"storage": "8gb"
	},
	"customizations": {
		"codespaces": {
			"openFiles": [
				"fft.cpp"
			]
		},
		"vscode": {
			"settings": {
				"window.showNotificationOnLoad": true
			},
			"extensions": [
     				"ms-vscode.cpptools",
				"ms-vscode.cpptools-extension-pack"
       			]
		}
	}
}
